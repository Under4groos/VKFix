{
  "name": "[RU] VKFix ",
  "version": "1.1",
  "description": "Удаляет лишние елементы в веб версии вк.",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "action": {
    "default_title": "VKFix",
    "default_popup": "index.html"
  },
  "icons": {
    "256": "images/favicon.png"
  }
   
}
